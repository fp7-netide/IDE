Content-Type: text/ecl
Entry-Name: .content

get-menu "Help/Install New Software..." | click
with [get-window Install] {
    get-combo -after [get-label "Work with:"]
        | set-text [concat [get-workspace-location]"/../../../releng/eu.netide.dependencies/target/repository"]
    get-button "Add..." | click
    with [get-window "Add Repository"] {
        get-editbox -after [get-label "Name:"] | set-text "NetIDE_dependencies"
        get-button OK | click
    }
    get-tree | get-item "Maven osgi-bundles" | check
    get-button "Next >" | click
    get-button "Next >" | click
    get-button "I accept the terms of the license agreements" | click
    get-button Finish | click
}
get-window "Security Warning" | get-button OK | click
wait-until-eclipse-is-ready
get-window "Software Updates" | get-button Yes | click
wait-until-eclipse-is-ready

get-menu "Help/Install New Software..." | click
with [get-window Install] {
    get-button "Add..." | click
    get-window "Add Repository" | get-editbox -after [get-label "Name:"] | set-text "NetIDE_Plugin"
}
set-dialog-result File [concat [get-workspace-location]"/../../../releng/eu.netide.product/target/eu.netide.product-0.4.0-SNAPSHOT.zip"]
with [get-window Install] {
    with [get-window "Add Repository"] {
        get-button "Archive..." | click
        get-button OK | click
    }
    get-tree | get-item NetIDE | check
    get-button "Next >" | click
    get-button "Next >" | click
    get-button "I accept the terms of the license agreements" | click
    get-button Finish | click
}

get-window "Security Warning" | get-button OK | click
wait-until-eclipse-is-ready
get-window "Software Updates" | get-button Yes | click
wait-until-eclipse-is-ready
