Content-Type: text/ecl
Entry-Name: .content

get-view "Package Explorer" | get-tree | select "Test_Project/Test_Project.wb" | get-menu "Open With/Other..." | click
get-window "Editor Selection" | get-tree | select "Workbench Configuration Editor" | double-click
get-editor "Test_Project.wb" | get-button "Create Profile" | click
with [get-window "SSH Profile"] {
    with [get-group "ssh Connection Settings"] {
        with [get-editbox -after [get-label "Profile Name"]] {
            set-text $ProfileName
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-editbox -after [get-label Username]] {
            set-text $Username
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-editbox -after [get-label Port]] {
            set-text $Port
            key-type "TRAVERSE_TAB_NEXT"
        }
        with [get-editbox -after [get-label "SSH ID File"]] {
            set-text $SSHIDFile
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-editbox -after [get-label Host] | set-text $Host
    }
    get-button "Save Profile" | click
}
get-editor "Test_Project.wb" | get-combo -after [get-label offline] | get-property "values[0]" | equals $ProfileName 
    | verify-true
get-editor "Test_Project.wb" | get-button "Edit Profile" | click
with [get-window "SSH Profile" | get-group "ssh Connection Settings"] {
    get-editbox -after [get-label "Profile Name"] | get-property text | equals $ProfileName | verify-true
    get-editbox -after [get-label Username] | get-property text | equals $Username | verify-true
    get-editbox -after [get-label Port] | get-property text | equals $Port | verify-true
    get-editbox -after [get-label "SSH ID File"] | get-property text | equals $SSHIDFile | verify-true
    get-editbox -after [get-label Host] | get-property text | equals $Host | verify-true
}
get-window "SSH Profile" | get-button Cancel | click
get-editor "Test_Project.wb" | get-combo -after [get-label offline] | get-property selection | equals $ProfileName 
    | verify-true

get-editor "Test_Project.wb" | close
get-window "Save Resource" | get-button Yes | click


get-view "Package Explorer" | get-tree | select "Test_Project/Test_Project.wb" | double-click
get-editor "Test_Project.wb" | get-button "Edit Profile" | click

with [get-window "SSH Profile" | get-group "ssh Connection Settings"] {
    get-editbox -after [get-label "Profile Name"] | get-property text | equals $ProfileName | verify-true
    get-editbox -after [get-label Username] | get-property text | equals $Username | verify-true
    get-editbox -after [get-label Port] | get-property text | equals $Port | verify-true
    get-editbox -after [get-label "SSH ID File"] | get-property text | equals $SSHIDFile | verify-true
    get-editbox -after [get-label Host] | get-property text | equals $Host | verify-true
}

get-window "SSH Profile" | get-button Cancel | click
