Content-Type: text/ecl
Entry-Name: .content

get-eclipse-window | key-type "M1+n"
with [get-window New] {
    get-editbox -after [get-label "Wizards:"] | set-text "type filter text"
    get-tree | select "NetIDE/NetIDE App"
    get-button "Next >" | click
}
with [get-window -class WizardDialog] {
    get-editbox -after [get-label "Name:"] | set-text NewRyuApp
    get-button Finish | click
}
with [get-view "Package Explorer" | get-tree] {
    select "NetIDE_Test_Project/apps" | double-click
    select "NetIDE_Test_Project/apps/NewRyuApp" | double-click
}
with [get-view "Package Explorer" | get-tree] {
    get-item "NetIDE_Test_Project/apps/NewRyuApp" | get-property caption | equals NewRyuApp | verify-true
    get-item "NetIDE_Test_Project/apps/NewRyuApp/NewRyuApp.params" | get-property caption | equals "NewRyuApp.params" 
        | verify-true
    get-item "NetIDE_Test_Project/apps/NewRyuApp/NewRyuApp.sysreq" | get-property caption | equals "NewRyuApp.sysreq" 
        | verify-true
}
