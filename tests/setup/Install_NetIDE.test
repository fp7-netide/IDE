Content-Type: text/ecl
Entry-Name: .content

set-q7-option "jobHangTimeout" "960000"
set-q7-option "jobSleepingStepTimeout" "1600000"
set-q7-option "jobSleepingStepTime" "1600"


get-menu "Help/Install New Software..." | click
with [get-window Install] {
    get-button "Add..." | click
    with [get-window "Add Repository"] {
        with [get-editbox -after [get-label "Name:"]] {
            set-text "netide"
            key-type "TRAVERSE_TAB_NEXT"
        }
        get-editbox -after [get-label "Location:"]
            | set-text [concat [get-workspace-location] "/../../../releng/eu.netide.product/target/repository"]
        get-button OK | click
    }
    get-tree | get-item "NetIDE" | check
    get-button "Next >" | click
    get-button "Next >" | click
    get-button "I accept the terms of the license agreements" | click

    get-button Finish | click
}
get-window "Software Updates" | get-button Yes | click
wait-until-eclipse-is-ready

